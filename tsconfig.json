{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@fonts/*": ["src/assets/fonts/*"],
      "@images/*": ["src/assets/images/*"],
      "@assets/*": ["src/assets/*"],
    }
  }
}
